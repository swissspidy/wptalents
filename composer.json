{
  "name"        : "swissspidy/wptalents",
  "description" : "Main Plugin for WPTalents.com",
  "keywords"    : [
    "wordpress",
    "plugin",
    "wptalents"
  ],
  "type"        : "wordpress-plugin",
  "homepage"    : "https://github.com/swissspidy/wptalents",
  "authors"     : [
    {
      "name"    : "Pascal Birchler",
      "email"   : "contact@spinpress.com",
      "homepage": "https://spinpress.com/"
    }
  ],
  "support"     : {
    "issues": "https://github.com/swissspidy/wptalents/issues"
  },
  "repositories": [
    {
      "type": "git",
      "url" : "https://github.com/swissspidy/Custom-Meta-Boxes"
    }
  ],
  "require"     : {
    "humanmade/custom-meta-boxes": "dev-post-type-check"
  },
  "autoload"    : {
    "psr-4": {
      "WPTalents\\API\\"      : "includes/api",
      "WPTalents\\CMB\\"      : "includes/cmb",
      "WPTalents\\Collector\\": "includes/collectors",
      "WPTalents\\Core\\"     : "includes/core",
      "WPTalents\\Types\\"    : "includes/types"
    }
  }
}