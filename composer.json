{
  "name": "swissspidy/wptalents",
  "description": "Main plugin for WP Talents",
  "keywords": [
    "wordpress",
    "plugin",
    "wptalents"
  ],
  "type": "wordpress-plugin",
  "homepage": "https://github.com/swissspidy/wptalents",
  "authors": [
    {
      "name": "Pascal Birchler",
      "email": "contact@spinpress.com",
      "homepage": "https://spinpress.com/"
    }
  ],
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "wp-coding-standards/wpcs": "dev-develop",
    "sebastian/phpcpd": "*"
  },
  "scripts": {
    "post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs",
    "post-update-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs"
  },
  "autoload": {
    "psr-4": {
      "WPTalents\\Lib\\": "lib",
      "WPTalents\\API\\": "classes/api",
      "WPTalents\\CLI\\": "classes/cli",
      "WPTalents\\Collector\\": "classes/collectors",
      "WPTalents\\Core\\": "classes/core",
      "WPTalents\\Types\\": "classes/types"
    }
  }
}